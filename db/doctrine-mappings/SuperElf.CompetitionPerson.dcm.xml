<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="SuperElf\CompetitionPerson" table="competitionPersons" repository-class="SuperElf\CompetitionPerson\Repository">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <many-to-one field="sourceCompetition" target-entity="Sports\Competition">
            <join-columns>
                <join-column name="sourceCompetitionId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="person" target-entity="Sports\Person">
            <join-columns>
                <join-column name="personId" referenced-column-name="id" nullable="false" on-delete="RESTRICT"/>
            </join-columns>
        </many-to-one>
        <one-to-many field="gameRoundScores" target-entity="SuperElf\CompetitionPerson\GameRoundScore" mapped-by="competitionPerson">
            <cascade>
                <cascade-remove/>
            </cascade>
        </one-to-many>
        <unique-constraints>
            <unique-constraint columns="sourceCompetitionId,personId" name="main_u_idx"/>
        </unique-constraints>
    </entity>
</doctrine-mapping>