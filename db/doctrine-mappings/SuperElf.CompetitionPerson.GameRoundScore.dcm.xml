<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SuperElf\CompetitionPerson\GameRoundScore" table="competitionPersonGameRoundScores" repository-class="SuperElf\CompetitionPerson\GameRoundScore\Repository">
    <many-to-one field="competitionPerson" target-entity="SuperElf\CompetitionPerson" inversed-by="gameRoundScores">
      <join-columns>
        <join-column name="competitionPersonId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <unique-constraints>
      <unique-constraint columns="competitionPersonId,gameRoundId" name="main_u_idx"/>
    </unique-constraints>
  </entity>
</doctrine-mapping>
