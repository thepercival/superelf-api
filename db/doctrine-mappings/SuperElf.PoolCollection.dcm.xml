<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SuperElf\PoolCollection" table="PoolCollections" repository-class="SuperElf\PoolCollection\Repository">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" column="name" type="string" length="20" nullable="false" unique="true"/>
    <one-to-many field="pools" target-entity="SuperElf\Pool" mapped-by="poolCollection">
      <cascade>
        <cascade-persist/>
        <cascade-remove/>
      </cascade>
    </one-to-many>
  </entity>

</doctrine-mapping>