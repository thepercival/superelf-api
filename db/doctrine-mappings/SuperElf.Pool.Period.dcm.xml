<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SuperElf\Pool\Period" table="poolPeriods" repository-class="SuperElf\Pool\Period\Repository">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="type" column="type" type="smallint" nullable="false" unique="false"/>
    <field name="startDateTime" column="startDateTime" type="datetime_immutable" nullable="false" unique="false"/>
    <field name="endDateTime" column="endDateTime" type="datetime_immutable" nullable="false" unique="false"/>
    <many-to-one field="pool" target-entity="SuperElf\Pool">
      <join-columns>
        <join-column name="poolId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>