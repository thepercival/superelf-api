<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="SuperElf\Achievement\Unviewed\Trophy" table="trophiesUnviewed" repository-class="SuperElf\Repositories\TrophyUnviewedRepository">
        <many-to-one field="trophy" target-entity="SuperElf\Achievement\Trophy">
            <join-columns>
                <join-column name="trophyId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
            </join-columns>
        </many-to-one>
    </entity>
</doctrine-mapping>
