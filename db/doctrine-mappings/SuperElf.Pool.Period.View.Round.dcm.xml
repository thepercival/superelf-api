<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SuperElf\Pool\Period\View\Round" table="poolViewPeriodRounds" repository-class="SuperElf\Pool\Period\View\Round\Repository">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="number" column="number" type="smallint" nullable="false" unique="false"/>
    <many-to-one field="poolViewPeriod" target-entity="SuperElf\Pool\Period\View" inversed-by="rounds">
      <join-columns>
        <join-column name="poolViewPeriodId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </many-to-one>
    <unique-constraints>
      <unique-constraint columns="poolViewPeriodId,number" name="main_u_idx"/>
    </unique-constraints>
  </entity>
</doctrine-mapping>