<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SuperElf\Period\Transfer\Transfer" table="transfers" repository-class="SuperElf\Transfer\Transfer\Repository">
    <many-to-one field="poolUser" target-entity="SuperElf\Pool\User" inversed-by="transfers">
      <cascade>
        <cascade-persist/>
        <cascade-remove/>
      </cascade>
      <join-columns>
        <join-column name="poolUserId" referenced-column-name="id" nullable="false" on-delete="RESTRICT"/>
      </join-columns>
    </many-to-one>
    <one-to-one field="personIn" target-entity="Sports\Person">
      <cascade>
        <cascade-persist/>
        <cascade-remove/>
      </cascade>
      <join-columns>
        <join-column name="peronInId" referenced-column-name="id" nullable="false" on-delete="CASCADE"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>